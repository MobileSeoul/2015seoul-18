<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="FullScreen" parent="android:Theme.Light.NoTitleBar.Fullscreen">

    </style>
    <style name="MenuButtonsSmall">
        <item name="fab_size">normal</item>
        <item name="fab_colorNormal">@color/colorPrimary</item>
        <item name="fab_colorPressed">@color/colorPrimaryDark</item>
        <item name="fab_colorRipple">@color/colorAccent</item>
    </style>
    <style name="MenuButtonsLarge">
        <item name="fab_colorNormal">@color/colorPrimary</item>
        <item name="fab_colorPressed">@color/colorPrimaryDark</item>
        <item name="fab_colorRipple">@color/colorAccent</item>
    </style>

    <style name="MenuButtonsSmall.FoodFab1" parent="AppTheme">
        <item name="fab_colorNormal">#fd9999</item>
        <item name="fab_colorPressed">#ff7575</item>
        <item name="fab_colorRipple">#c52525</item>
    </style>
